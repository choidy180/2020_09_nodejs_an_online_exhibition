<style>
    a{
        color: black;
    }
</style>
<div id="mypage_area">
    <div id="mypage_container">
        <% if (row[0] == undefined){%>
            <div class="inquiry_list_title" style="margin-top: 300px;">
                문의 내역이 존재하지 않습니다.
            </div>
        <%} else {%>
        <div class="inquiry_list_title">
            문의 리스트
        </div>
        <div class="inquiry_list_box">
            <div class="inquiry_list_nav">
                <div class="inquiry_list_nav_box1"></div>
                <div class="inquiry_list_nav_box2">제목</div>
                <div class="inquiry_list_nav_box3">작성일</div>
                <div class="inquiry_list_nav_box4">답변여부</div>
            </div>
            <%for (var i = 0; i < row.length; i++){%>
            <div class="inquiry_list_nav" style="border-top: 0;">
                <div class="inquiry_list_nav_box1"><%=row[i].NUMBER%></div>
                <%if(row[i].ANSWER_WHETHER == 'N'){%>
                    <div class="inquiry_list_nav_box2" style="justify-content: flex-start;" style="cursor: pointer;"><%=row[i].TITLE%></div>
                <%} else {%>
                    <div class="inquiry_list_nav_box2a" style="justify-content: flex-start;"><a href="/inquiry_list_yes/<%=row[0].NUMBER%>"><%=row[i].TITLE%></a></div>
                <%}%>
                <div class="inquiry_list_nav_box3"><%=register_date[0]%></div>
                <%if(row[i].ANSWER_WHETHER == 'N'){%>
                    <div class="inquiry_list_nav_box4" style="font-weight: 700; color: red;">N</div>
                <%} else {%>
                    <div class="inquiry_list_nav_box4" style="font-weight: 700; color: #1387fc;">Y</div>
                <%}%>
            </div>
            <%}%>
        </div>
        <%}%>
    </div>
</div>