<style>
    a{
        color: black;
    }
</style>
<div class="app_for_exhibition_container_right" style="height: auto; margin-bottom: 20px;" >
    <div id="mypage_area" style="width: 100%; padding-left: 20px; justify-content: flex-start;">
        <div id="mypage_container" style="width: 900px">
            <% if (row[0] == undefined){%>
                <div class="inquiry_list_title" style="margin-top: 300px;">
                    문의 내역이 존재하지 않습니다.
                </div>
            <%} else {%>
            <div class="inquiry_list_title">
                미답변 문의 리스트
            </div>
            <div class="inquiry_list_box" style="width: 700px;">
                <div class="inquiry_list_nav" style="width: 700px;">
                    <div class="inquiry_list_nav_box1"></div>
                    <div class="inquiry_list_nav_box2" style= "width: 400px;">제목</div>
                    <div class="inquiry_list_nav_box3">작성일</div>
                    <div class="inquiry_list_nav_box4">답변여부</div>
                </div>
                <%for (var i = 0; i < row.length; i++){%>
                <div class="inquiry_list_nav" style="border-top: 0; width: 700px; ">
                    <div class="inquiry_list_nav_box1"><%=row[i].NUMBER%></div>
                    <div class="inquiry_list_nav_box2a" style="justify-content: flex-start; width: 400px;"><a href="/inquiry_answer_detail/<%=row[i].NUMBER%>"><%=row[i].TITLE%></a></div>
                    <div class="inquiry_list_nav_box3"><%=register_date[i]%></div>
                    <%if(row[i].ANSWER_WHETHER == 'N'){%>
                        <div class="inquiry_list_nav_box4" style="font-weight: 700; color: red;">N</div>
                    <%} else {%>
                        <div class="inquiry_list_nav_box4" style="font-weight: 700; color: #1387fc;">Y</div>
                    <%}%>
                </div>
                <%}%>
            </div>
            <%}%>
        </div>
    </div>
</div>