<div id="mypage_area">
    <div id="mypage_container" style="margin-top: 100px; width: 900px;">
        <div class="book_mark_title">
            즐겨찾는 작가
        </div>
        <div class="book_mark_content_box" style="margin-top: 80px; justify-content: center;" >
            <%for (var i = 0; i < row.length; i++){%>
                <div class="book_mark_content">
                    <div class="book_mark_content_img">
                        <img src="/uploads/<%=row[i].IMG%>" alt="">
                    </div>
                    <div class="bookmark_text_box">
                        <div class="bookmark_title_text">
                            <%=row[i].NAME%>
                        </div>
                        <div class="bookmark_start_box">
                            <button onclick="location.href=''">삭제</button>
                        </div>
                    </div>
                </div>
            <%}%>
        </div>
    </div>
</div>